{
  "name": "react-laravel-auth",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "description": "Authentication template for React and Laravel Sanctum",
  "main": "./dist/react-laravel-auth.umd.js",
  "module": "./dist/react-laravel-auth.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-laravel-auth.es.js",
      "require": "./dist/react-laravel-auth.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shenaldev/React-Laravel-Auth-Template.git"
  },
  "keywords": [
    "react",
    "laravel",
    "sanctum",
    "template"
  ],
  "author": "Shenal Fernando",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shenaldev/React-Laravel-Auth-Template/issues"
